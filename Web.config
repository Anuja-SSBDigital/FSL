      </buildProviders>
    </compilation>
    <pages enableEventValidation="false"/>
  </system.web>
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None"/>
  </appSettings>
  <system.net>
    <mailSettings>
      <smtp deliveryMethod="Network">
        <network host="smtp.gmail.com" port="587" userName="skill4sale308@gmail.com" password="Pass@123" enableSsl="true"/>
      </smtp>
    </mailSettings>
  </system.net>
  <system.webServer>
